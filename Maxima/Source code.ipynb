       "\\PY{+w}{                    }\\PY{k}{for}\\PY{+w}{ }\\PY{n+nv}{i}\\PY{o}{:}\\PY{n+nv}{dim}\\PY{+w}{ }\\PY{k}{thru}\\PY{+w}{ }\\PY{n+nf}{length}\\PY{p}{(}\\PY{n+nv}{dims}\\PY{p}{)}\\PY{+w}{ }\\PY{k}{do}\n",
       "\\PY{+w}{                    }\\PY{p}{(}\n",
       "\\PY{+w}{                        }\\PY{n+nv}{t}\\PY{o}{:}\\PY{+w}{ }\\PY{n+nf}{split\\PYZus{}data}\\PY{p}{(}\\PY{n+nv}{input}\\PY{p}{,}\\PY{+w}{ }\\PY{n+nv}{dims}\\PY{p}{[}\\PY{n+nv}{i}\\PY{p}{]}\\PY{p}{)}\\PY{p}{,}\n",
